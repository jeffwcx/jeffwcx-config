{
  "name": "@jeffwcx/config",
  "version": "0.0.0",
  "private": "true",
  "description": "Shared configuration for jeffwcx",
  "keywords": [
    "config",
    "Prettier",
    "ESLint",
    "gitignore"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffwcx/jeffwcx-config.git"
  },
  "license": "MIT",
  "author": "jeffwcx <jeffwcx@icloud.com>",
  "type": "module",
  "scripts": {
    "changeset": "changeset",
    "format": "eslint . --cache --fix",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --cache",
    "release": "changeset publish",
    "typecheck": "tsc --noEmit",
    "version-pkg": "changeset version"
  },
  "prettier": "@jeffwcx/prettier-config",
  "dependencies": {
    "oclif": "^4.10.2",
    "tsx": "^4.9.1"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@jeffwcx/eslint-config": "workspace:*",
    "@jeffwcx/prettier-config": "workspace:*",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/node": "^20.12.7",
    "eslint": "^9.26.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  }
