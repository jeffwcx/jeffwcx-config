{
  "name": "@jeffwcx/license",
  "version": "0.1.1",
  "description": "Generate license file.",
  "keywords": [
    "generator",
    "gitignore"
  ],
  "homepage": "https://github.com/jeffwcx/jeffwcx-config/blob/main/packages/license#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffwcx/jeffwcx-config.git",
    "directory": "packages/license"
  },
  "license": "MIT",
  "author": "jeffwcx <jeffwcx@icloud.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lic": "./bin/run.js",
    "license": "./bin/run.js"
  },
  "files": [
    "bin",
    "dist",
    "license-list-data/json",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "tsup",
    "data-update": "git submodule update --remote --rebase",
    "dev": "tsup --watch",
    "docs": "oclif manifest && oclif readme",
    "lic": "tsx ./bin/dev.js",
    "lic:debug": "env DEBUG=* tsx ./bin/dev",
    "prepack": "pnpm build && pnpm run docs"
  },
  "oclif": {
    "bin": "lic",
    "commands": {
      "strategy": "single",
      "target": "./dist/index.js"
    }
  },
  "dependencies": {
    "@inquirer/figures": "^1.0.1",
    "@inquirer/input": "^2.1.8",
    "@oclif/core": "^3.26.6",
    "chalk": "^5.3.0",
    "flexsearch": "^0.7.43",
    "inquirer-select-pro": "1.0.0-alpha.4",
    "ora": "^8.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
